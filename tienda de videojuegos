  `pStatus` varchar(45) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `pPrecio` float NOT NULL,
  `pStock` int NOT NULL,
  `pFoto` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT 'hi',
  `pDisponible` tinyint(1) NOT NULL,
  PRIMARY KEY (`idProducto`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 1032 CHARACTER SET = utf8 COLLATE = utf8_general_ci ROW_FORMAT = Dynamic;

