CREATE TABLE `producto`  (
  `idProducto` int NOT NULL AUTO_INCREMENT,
  `pNombre` varchar(80) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `pTipo` varchar(45) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `pConsola` varchar(45) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
