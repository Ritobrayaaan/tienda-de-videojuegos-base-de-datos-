-- ----------------------------
-- Table structure for detallefactura
-- ----------------------------
DROP TABLE IF EXISTS `detallefactura`;
CREATE TABLE `detallefactura`  (
  `Venta_idVenta` int NOT NULL,
  `Factura_idFactura` int NOT NULL,
  PRIMARY KEY (`Venta_idVenta`, `Factura_idFactura`) USING BTREE,
  INDEX `fk_Venta_has_Factura_Factura1_idx`(`Factura_idFactura` ASC) USING BTREE,
  INDEX `fk_Venta_has_Factura_Venta1_idx`(`Venta_idVenta` ASC) USING BTREE,
  CONSTRAINT `fk_Venta_has_Factura_Factura1` FOREIGN KEY (`Factura_idFactura`) REFERENCES `factura` (`idFactura`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_Venta_has_Factura_Venta1` FOREIGN KEY (`Venta_idVenta`) REFERENCES `venta` (`idVenta`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE = InnoDB CHARACTER SET = utf8 COLLATE = utf8_general_ci ROW_FORMAT = Dynamic;

-- ----------------------------
-- Records of detallefactura
-- ----------------------------

-- ----------------------------
-- Table structure for detalleventa
-- ----------------------------
DROP TABLE IF EXISTS `detalleventa`;
CREATE TABLE `detalleventa`  (
  `Producto_idProducto` int NOT NULL,
  `Venta_idVenta` int NOT NULL,
  `Cantidad` int NULL DEFAULT NULL,
  `Precio` float NULL DEFAULT NULL,
  PRIMARY KEY (`Producto_idProducto`, `Venta_idVenta`) USING BTREE,
  INDEX `fk_Producto_has_Venta_Venta1_idx`(`Venta_idVenta` ASC) USING BTREE,
  INDEX `fk_Producto_has_Venta_Producto1_idx`(`Producto_idProducto` ASC) USING BTREE,
  CONSTRAINT `fk_Producto_has_Venta_Producto1` FOREIGN KEY (`Producto_idProducto`) REFERENCES `producto` (`idProducto`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_Producto_has_Venta_Venta1` FOREIGN KEY (`Venta_idVenta`) REFERENCES `venta` (`idVenta`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE = InnoDB CHARACTER SET = utf8 COLLATE = utf8_general_ci ROW_FORMAT = Dynamic;

