-- ----------------------------
-- Table structure for factura
-- ----------------------------
DROP TABLE IF EXISTS `factura`;
CREATE TABLE `factura`  (
  `idFactura` int NOT NULL AUTO_INCREMENT,
  `fFecha` datetime NOT NULL,
  `fDisponible` tinyint(1) NOT NULL,
  `venta_idVenta` int NOT NULL,
  PRIMARY KEY (`idFactura`, `venta_idVenta`) USING BTREE,
  INDEX `fk_factura_venta1_idx`(`venta_idVenta` ASC) USING BTREE,
  CONSTRAINT `fk_factura_venta1` FOREIGN KEY (`venta_idVenta`) REFERENCES `venta` (`idVenta`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE = InnoDB AUTO_INCREMENT = 8 CHARACTER SET = utf8 COLLATE = utf8_general_ci ROW_FORMAT = Dynamic;

