-- ----------------------------
-- Table structure for bitacora_del
-- ----------------------------
DROP TABLE IF EXISTS `bitacora_del`;
CREATE TABLE `bitacora_del`  (
  `id_bitacora` int NOT NULL AUTO_INCREMENT,
  `Id` int NOT NULL,
  `fecha` timestamp NOT NULL DEFAULT current_timestamp ON UPDATE CURRENT_TIMESTAMP,
  `ejecutor` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL,
  `actividad` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL,
  `informacion_actual` text CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL,
  `informacion` int NOT NULL,
  PRIMARY KEY (`id_bitacora`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 3 CHARACTER SET = utf8mb4 COLLATE = utf8mb4_general_ci ROW_FORMAT = Dynamic;

