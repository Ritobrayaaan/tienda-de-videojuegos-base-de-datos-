-- ----------------------------
-- View structure for capellido
-- ----------------------------
DROP VIEW IF EXISTS `capellido`;
CREATE ALGORITHM = UNDEFINED SQL SECURITY DEFINER VIEW `capellido` AS select `cliente`.`idCliente` AS `idCliente`,`cliente`.`cEmail` AS `cEmail`,`cliente`.`cTel` AS `cTel` from `cliente` where `cliente`.`cApellido` = ' Hernández';

-- ----------------------------
-- View structure for cnombre
-- ----------------------------
DROP VIEW IF EXISTS `cnombre`;
CREATE ALGORITHM = UNDEFINED SQL SECURITY DEFINER VIEW `cnombre` AS select `cliente`.`idCliente` AS `idCliente`,`cliente`.`cEmail` AS `cEmail`,`cliente`.`cTel` AS `cTel` from `cliente` where `cliente`.`cEmail` = ' cApellido';

-- ----------------------------
-- Triggers structure for table producto
-- ----------------------------
DROP TRIGGER IF EXISTS `producto_int`;
delimiter ;;
CREATE TRIGGER `producto_int` AFTER INSERT ON `producto` FOR EACH ROW BEGIN

INSERT INTO
bitacora(id,ejecutor,actividad,informacion_actual)VALUES(
    NEW.idProducto,CURRENT_USER,'Se inserto nuevo producto',concat('Informacion actual: ',NEW.pNombre,'' ,NEW.pTipo,'' ,NEW.pGenero,''));
END
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table producto
-- ----------------------------
DROP TRIGGER IF EXISTS `producto_dele`;
delimiter ;;
CREATE TRIGGER `producto_dele` AFTER DELETE ON `producto` FOR EACH ROW BEGIN

INSERT INTO
bitacora_del(id,ejecutor,actividad,informacion_actual)VALUES(
    OLD.idProducto,CURRENT_USER,'Se inserto nuevo producto',concat('Informacion actual: ',OLD.pNombre,'' ,OLD.pTipo,'' ,OLD.pGenero,''));
END
;;
delimiter ;

SET FOREIGN_KEY_CHECKS = 1;
