-- ----------------------------
-- Table structure for venta
-- ----------------------------
DROP TABLE IF EXISTS `venta`;
CREATE TABLE `venta`  (
  `idVenta` int NOT NULL AUTO_INCREMENT,
  `vFecha` datetime NOT NULL,
  `vTipoPago` varchar(45) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `VC` varchar(45) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `vDisponible` tinyint(1) NOT NULL,
  `vSaldo` float NOT NULL DEFAULT 0,
  `Empleado_idEmpleado` int NOT NULL,
  `Cliente_idCliente` int NOT NULL,
  PRIMARY KEY (`idVenta`) USING BTREE,
  INDEX `fk_Venta_Empleado_idx`(`Empleado_idEmpleado` ASC) USING BTREE,
  INDEX `fk_Venta_Cliente1_idx`(`Cliente_idCliente` ASC) USING BTREE,
  CONSTRAINT `fk_Venta_Cliente1` FOREIGN KEY (`Cliente_idCliente`) REFERENCES `cliente` (`idCliente`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_Venta_Empleado` FOREIGN KEY (`Empleado_idEmpleado`) REFERENCES `empleado` (`idEmpleado`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE = InnoDB AUTO_INCREMENT = 15 CHARACTER SET = utf8 COLLATE = utf8_general_ci ROW_FORMAT = Dynamic;

